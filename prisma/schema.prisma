generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  adapter  = "pg"
}

model Audit {
  id        String   @id @default(cuid())
  url       String
  results   Json     // LH scores, custom checks
  summary   String?  // AI insights
  createdAt DateTime @default(now())
  userId    String?
}
